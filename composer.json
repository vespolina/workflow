{
    "name": "vespolina/workflow",
    "description": "Vespolina library for handling workflow",
    "homepage": "http://vespolina-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Vespolina Team",
            "homepage": "https://github.com/vespolina/workflow/contributors"
        }
    ],
    "keywords": ["Vespolina", "workflow"],
    "require": {
        "php": ">=5.3.0",
        "psr/log": "1.0.0"
    },
    "require-dev": {
        "bernard/bernard": ">=0.11.0,<1.0.0",
        "phpspec/phpspec": "2.0.*@dev",
        "monolog/monolog": "1.6.*",
        "raulfraile/ladybug": "~1.0"
    },
    "suggest": {
        "bernard/bernard": ">=0.11.0,<1.0.0"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Vespolina": "lib/",
            "Vespolina\\Tests": "tests/"
        }
    }
}
